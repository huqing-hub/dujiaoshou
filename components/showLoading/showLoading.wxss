/* components/showLoading/showLoading.wxss */
.showLoading{position: fixed;width:100%;height:100%;z-index: 9999;left:0;top:0;}
.showLoading .box{width:200rpx;height:200rpx;background: rgba(0,0,0,.7);border-radius: 10rpx;flex-direction: column;}
.showLoading, .showLoading .box{display: flex;align-items: center;justify-content: center;}
.showLoading .box image{width:40%;}
.showLoading .box .text{color:#fff;font-size:28rpx;padding-top: 10rpx;}