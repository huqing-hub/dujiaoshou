/* pages/memberShop/memberShop.wxss */
.whole{
	padding-bottom: 20rpx;
}
.whole>.shoplist:not(:first-child){
	padding-top: 30rpx;
}
.shoplist{
	width: 690rpx;
	margin: 0 auto;
	margin-top: 20rpx;
}
.shopimg{
	width: 100%;
	height:320rpx;
	border-radius:10rpx;
	background: center no-repeat;
	background-size: cover;
}
.shopName{
	font-size:30rpx;
	font-family:PingFang SC;
	font-weight:bold;
	color:rgba(49,49,52,1);
	margin-top: 30rpx;
	line-height: 36rpx;
}
.shopPro{
	margin-top: 18rpx;
	font-size:22rpx;
	font-family:PingFang SC;
	font-weight:500;
	color:rgba(148,151,163,1);
}
.shopFoot{
	width: 100%;
	display: flex;
	justify-content: space-between;
	margin-top: 39rpx;
}
.txt1{
	font-size:22rpx;
	font-family:PingFang SC;
	font-weight:500;
	color:rgba(148,151,163,1);
}
.txt2{
	font-size:26rpx;
	font-family:PingFang SC;
	font-weight:bold;
	color:#FFA398;
}